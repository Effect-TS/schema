{
  "$schema": "node_modules/@effect/docgen/schema.json",
  "exclude": ["src/internal/**/*.ts"],
  "parseCompilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "ES2021",
    "lib": ["ES2021"],
    "paths": {
      "@effect/schema": ["./src/index.ts"],
      "@effect/schema/test/*": ["./test/*"],
      "@effect/schema/examples/*": ["./examples/*"],
      "@effect/schema/*": ["./src/*"]
    }
  },
  "examplesCompilerOptions": {
    "strict": true,
    "noEmit": true,
    "target": "ES2021",
    "lib": ["ES2021"],
    "paths": {
      "@effect/schema": ["../../src/index.ts"],
      "@effect/schema/test/*": ["../../test/*"],
      "@effect/schema/examples/*": ["../../examples/*"],
      "@effect/schema/*": ["../../src/*"]
    }
  }
}
